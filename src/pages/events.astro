---
import StarlightPage from '@astrojs/starlight/components/StarlightPage.astro';
import { LinkCard } from '@astrojs/starlight/components';

const pages = await Astro.glob('../content/docs/events/*');
---

<StarlightPage frontmatter={{ title: 'LSL Events' }}>
	<div class="category-links">
	{pages.map(page =>
		<a href={page.frontmatter.slug || page.url.replace('src/content/docs', '').replace('.mdx', '')}>
			{page.frontmatter.title}
		</a>
	)}
	</div>
</StarlightPage>
